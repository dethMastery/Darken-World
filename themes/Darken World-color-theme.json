// Scheme
// 	Rich Black FOGRA 29	- 0D1B2A
// 	Oxford Blue			- 1B263B
// 	Bdazzled Blue		- 415A77
// 	Shadow Blue			- 778DA9
// 	Platinum 			- E0E1DD

{
	"name": "Darken World",
	"type": "dark",
	"colors": {
		// General
			"focusBorder": "#1B263B",

		// Title
			// Active BG
				"titleBar.activeBackground": "#0D1B2A",
			// Active FG
				"titleBar.activeForeground": "#E0E1DD",
			// InActive BG
				"titleBar.inactiveBackground": "#152B44",
			// InActive FG
				"titleBar.inactiveForeground": "#E0E1DD",
				
		// Tab
			// Active Border
				"tab.activeBorderTop": "#415A77",
			// Border
				"tab.border": "#415A77",
			// InActive BG
				"tab.inactiveBackground": "#778DA9",
			// InActive FG
				"tab.inactiveForeground": "#778DA9",

		// Editor
			// Background
				"editor.background": "#0D1B2A",
			// Text
				"editor.foreground": "#E0E1DD",
			// Holding
				"editor.selectionBackground": "#415A77",

			// Front Column Line
				"editorWhitespace.foreground": "#778DA9",

			// Another Holding
				"selection.background": "#415A77",

		// Menu
			// Background
				"menu.background": "#0D1B2A",
			// Selection BG
				"menu.selectionBackground": "#1B263B",
			// Selection FG
				"menu.selectionForeground": "#778DA9",
		
		// Menubar
			// Selection BG
				"menubar.selectionBackground": "#1B263B",
			// Selection FG
				"menubar.selectionForeground": "#778DA9",

		// Sidebar
			"sideBarTitle.foreground": "#E0E1DD",

	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#81B4FF"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#E0E1DD"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#E0E1DD"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF3737"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#D29DF5"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#81B4FF"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#B60000"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#81B4FF"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#CAD1E4"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#FF3737"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#FF8B8B",
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
